<templateSet group="ActionScript">
  <template name="iter" value="for each (var $VAR$:$ELEMENT_TYPE$ in $ARRAY$) {&#10;  $END$&#10;}" description="迭代 (for each..in)" toReformat="true" toShortenFQNames="true">
    <variable name="ARRAY" expression="jsArrayVariable()" defaultValue="&quot;array &quot;" alwaysStopAt="true" />
    <variable name="ELEMENT_TYPE" expression="jsComponentTypeOf(ARRAY)" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="&quot;o&quot;" alwaysStopAt="true" />
  </template>
  <template name="itin" value="for (var $VAR$:String in $ARRAY$) {&#10;  $END$&#10;}" description="迭代 (for..in)" toReformat="true" toShortenFQNames="true">
    <variable name="ARRAY" expression="jsArrayVariable()" defaultValue="&quot;array &quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="&quot;o&quot;" alwaysStopAt="true" />
  </template>
  <template name="itar" value="for (var $INDEX$:int = 0; $INDEX$ &lt; $ARRAY$.length; $INDEX$++) {&#10;  var $VAR$:$ELEMENT_TYPE$ = $ARRAY$[$INDEX$];&#10;  $END$&#10;}" description="遍历数组元素" toReformat="true" toShortenFQNames="true">
    <variable name="INDEX" expression="jsSuggestIndexName()" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="ARRAY" expression="jsArrayVariable()" defaultValue="&quot;array&quot;" alwaysStopAt="true" />
    <variable name="ELEMENT_TYPE" expression="jsComponentTypeOf(ARRAY)" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="&quot;o&quot;" alwaysStopAt="true" />
  </template>
  <template name="ritar" value="for (var $INDEX$:int = $ARRAY$.length - 1; $INDEX$ &gt;= 0; $INDEX$--) {&#10;  var $VAR$:$ELEMENT_TYPE$ = $ARRAY$[$INDEX$];&#10;  $END$&#10;}" description="反序遍历数组元素" toReformat="true" toShortenFQNames="true">
    <variable name="INDEX" expression="jsSuggestIndexName()" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="ARRAY" expression="jsArrayVariable()" defaultValue="&quot;array&quot;" alwaysStopAt="true" />
    <variable name="ELEMENT_TYPE" expression="jsComponentTypeOf(ARRAY)" defaultValue="&quot;Object&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="&quot;o&quot;" alwaysStopAt="true" />
  </template>
</templateSet>